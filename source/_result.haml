.row{id: snakecase(area)}
  %h3= area.titleize

  - result.each do |title, isps |

    .col-lg-2
      %h4= title.titleize

    .col-lg-10
      %table.table
        %thead
          %tr
            %th ISP
            %th Ping
        %tbody
          - result.game_latencies["diablo"].each do |name, ping|
            %tr{class: ping_class(average(ping))}
              %td= name.titleize
              %td= average(ping)

